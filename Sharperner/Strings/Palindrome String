/**
 * @param {string} S
 * @return {number}
*/

class Solution {
    
    isPalindrome(S){
        
        let i=0, j= S.length-1;
        
        while(i<j) {
            if(S[i] !== S[j]) {
                return 0;
            }
            i++;
            j--;
        }
        
        return 1;
    }
}

Time Complexity --> O(LogN);
Space Complexity --> O(1);
